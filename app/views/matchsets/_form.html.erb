<%= form_for(matchset) do |f| %>
  <% if matchset.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(matchset.errors.count, "error") %> prohibited this matchset from being saved:</h2>

      <ul>
      <% matchset.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :song_name %>
    <%= f.text_field :song_name %>
  </div>

  <div class="field">
    <%= f.label :picked_player_id %>
    <%= f.number_field :picked_player_id %>
  </div>

  <div class="field">
    <%= f.label :player1_score %>
    <%= f.number_field :player1_score %>
  </div>

  <div class="field">
    <%= f.label :player2_score %>
    <%= f.number_field :player2_score %>
  </div>

  <div class="field">
    <%= f.label :match_id %>
    <%= f.number_field :match_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
